{
    "env": {
        "browser": true,
        "node":true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "angular": 1
    },
    "rules": {
        "curly": [2, "all"],
        "eqeqeq": [2, "allow-null"],
        "guard-for-in": [2],
        "wrap-iife": [2, "inside"],
        "complexity": [2, 10],
        "max-depth": [2, 5],
        "new-cap": [2, {"capIsNew": false}],
        "no-caller": [2],
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-trailing-spaces": [2],
        "no-undef": [2],
        "no-unused-vars": [2],
        "semi": [2, "always"],
        "dot-notation": [2],
        "no-alert": [2],
        "no-console": [2],
        "no-debugger": [2],
        "no-cond-assign": [2],
        "no-constant-condition": [2],
        "no-control-regex": [2],
        "no-div-regex": [2],
        "no-regex-spaces": [2],
        "no-invalid-regexp": [2],
        "no-empty-character-class": [2],
        "no-dupe-args": [2],
        "no-dupe-keys": [2],
        "no-duplicate-case": [2],
        "no-empty": [2],
        "no-ex-assign": [2],
        "no-extra-boolean-cast": [2],
        "no-irregular-whitespace": [2],
        "no-negated-in-lhs": [2],
        "no-obj-calls": [2],
        "no-sparse-arrays": [2],
        "no-unreachable": [2],
        "use-isnan": [2],
        "valid-typeof": [2],
        "no-unexpected-multiline": [2],
        "block-scoped-var": [2],
        "dot-location": [2, "property"],
        "yoda": [2, "never"],
        "no-with": [2],
        "no-useless-concat": [2],
        "no-useless-call": [2],
        "no-octal": [2],
        "no-redeclare": [2, {"builtinGlobals": true}],
        "no-eval": [2],
        "no-implied-eval": [2],
        "no-delete-var": [2],
        "no-extra-semi": [2],
        "no-inner-declarations": [2],
        "no-fallthrough": [2],
        "no-extend-native": [2],
        "no-floating-decimal": [2],
        "no-iterator": [2],
        "no-labels": [2],
        "no-lone-blocks": [2],
        "no-loop-func": [2],
        "no-multi-str": [2],
        "no-native-reassign": [2],
        "no-new-func": [2],
        "no-octal-escape": [2],
        "no-new-wrappers": [2],
        "no-proto": [2],
        "no-return-assign": [2, "always"],
        "no-script-url": [2],
        "no-self-compare": [2],
        "no-sequences": [2],
        "no-throw-literal": [2],
        "no-unused-expressions": [2],
        "no-shadow-restricted-names": [2],
        "no-label-var": [2],
        "no-catch-shadow": [2],
        "array-bracket-spacing": [2, "never"],
        "block-spacing": [2, "always"],
        "brace-style": [2],
        "camelcase": [2, {"properties": "always"}],
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "consistent-this": [2, "self"],
        "eol-last": [2],
        "func-style": [0, "declaration"],
        "key-spacing": [2],
        "no-lonely-if": [2],
        "no-multiple-empty-lines": [2],
        "no-nested-ternary": [2],
        "no-spaced-func": [2],
        "one-var": [2, "never"],
        "operator-assignment": [2, "always"],
        "quote-props": [2, "as-needed"],
        "semi-spacing": [2],
        "keyword-spacing": [2],
        "space-before-blocks": [2, "always"],
        "space-before-function-paren": [2, "never"],
        "space-in-parens": [2, "never"],
        "comma-dangle": [2, "never"],
        "no-shadow": [2],
        "no-undefined": [2],
        "no-eq-null": [2],
        "no-extra-bind": [2],
        "no-else-return": [2],
        "no-multi-spaces": [2],
        "no-new": [2],
        "no-param-reassign": [2],
        "no-use-before-define": [2, "nofunc"],
        "strict": [0, "never"],
        "prefer-const": [2],
        "no-var": [2],
        "prefer-template": [2],
        "object-shorthand": [2],
        "no-this-before-super": [2],
        "no-duplicate-imports": [2]
    }
}
